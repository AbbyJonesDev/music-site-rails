<p id="notice"><%= notice %></p>

<p>
  <b>Position:</b>
  <%= @song.position %>
</p>

<p>
  <b>Title:</b>
  <%= @song.title %>
</p>

<p>
  <b>Mp3:</b>
  <%= @song.mp3 %>
</p>

<p>
  <b>Ogg:</b>
  <%= @song.ogg %>
</p>

<p>
  <b>Description:</b>
  <%= @song.description %>
</p>

<% if admin_signed_in? %>
  <%= link_to 'Edit', edit_song_path(@song) %> |
  <%= link_to 'Back', songs_path %>
<% else %>
  <%= link_to 'Back', root_path %>
<% end %>


